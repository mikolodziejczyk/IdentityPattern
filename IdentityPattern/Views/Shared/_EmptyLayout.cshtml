<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @{Html.RenderPartial("_DefaultHeadContent");}
    @RenderSection("AdditionalHead", required: false)
</head>
<body>
    <div class="container-fluid">
        <div class="row">
            @RenderSection("PageHeader", required: false)
        </div>
        @RenderBody()
    </div>
    @{Html.RenderPartial("_DefaultFinalScripts");}
</body>
</html>
